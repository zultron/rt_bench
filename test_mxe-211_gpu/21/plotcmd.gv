set terminal png
set output "test_mxe-211_gpu/21/plot-21.png"
set multiplot layout 2, 1 title "Latency histogram:  glmark2 buffer:columns=200:interleave=true:update-dispersion=0.9:update-fraction=0.5:update-method=map" font ",14"
unset xtics
numcpus = 2
top_border = (1 - 0.1)
bot_border = 0.05
inter_border = 0.02
graph_height = (top_border - bot_border - inter_border * (numcpus - 1)) / numcpus
graph_dist = (top_border - bot_border) / numcpus
graphnum = 1
set tmargin at screen (top_border - (graphnum-1) * graph_dist)
set bmargin at screen (top_border - (graphnum-1) * graph_dist - graph_height)
set xrange [0:400]
set logscale y
set ytics font ",8"
set yrange [0.8:*]
set ylabel " "
set ylabel "Number of latency samples"
plot "test_mxe-211_gpu/21/histogram1" using 1:2 title "CPU2: max 00083" with histeps
#
graphnum = 2
set tmargin at screen (top_border - (graphnum-1) * graph_dist)
set bmargin at screen (top_border - (graphnum-1) * graph_dist - graph_height)
set xrange [0:400]
set logscale y
set ytics font ",8"
set yrange [0.8:*]
set ylabel " "
set xlabel "Latency (us)"
set xtics nomirror
plot "test_mxe-211_gpu/21/histogram2" using 1:2 title "CPU3: max 00065" with histeps
#
unset multiplot
